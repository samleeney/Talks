\frametitle{JAX numpy vs numpy}
\begin{columns}
    \column{0.5\textwidth}
    \begin{verbatim}
    import numpy as np
    x = np.arange(10)
    y = np.zeros_like(x)
    for i in range(len(x)):
        y[i] = x[i] * 2
    \end{verbatim}
    \column{0.5\textwidth}
    \begin{verbatim}
    import jax.numpy as jnp
    from jax import vmap
    x = jnp.arange(10)
    def double(v):
        return v * 2
    y = vmap(double)(x)
    \end{verbatim}
  \end{columns}
